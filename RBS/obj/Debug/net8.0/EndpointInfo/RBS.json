{
  "openapi": "3.0.1",
  "info": {
    "title": "RBS",
    "version": "1.0"
  },
  "paths": {
    "/api/User/register": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddUser"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddUser"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AddUser"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
